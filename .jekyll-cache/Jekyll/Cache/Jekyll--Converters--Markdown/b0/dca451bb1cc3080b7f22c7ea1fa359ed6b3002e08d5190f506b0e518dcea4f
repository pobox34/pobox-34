        var facets = Array.from(new Set(['may-29-2020', 'july-7-2020', 'may-29-2020', 'may-27-2020', 'june-2-2020', 'july-1-2020', 'july-23-2020', 'july-23-2020', 'july-23-2020', 'may-29-2020', 'may-26-2020', 'april-29-2020', 'june-2-2020', 'may-27-2020', 'july-2-2020', 'december-22-2020', 'january-14-2021', 'january-15-2021', 'march-15-2021', 'march-15-2021', 'march-15-2021', 'may-22-2020', 'march-15-2021', '2020', 'february-16-2021', '2021', 'december-14-2020', 'february-28-2021', 'february-28-2021', 'march-21-2021', 'february-28-2021', 'january-19-2021', 'june-3-2020', 'february-28-2021', 'may-20-2021', 'april-11-2021', 'january-19-2021', 'may-20-2021', 'january-19-2021', 'march-28-2021', 'february-24-2021', 'may-20-2021', 'may-20-2021', 'june-3-2020', 'march-7-2021', 'april-14-2021', 'february-17-2021', 'february-23-2021', 'june-3-2020', 'june-3-2020', 'may-26-2020']));
